{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-parens": [2, "always"],
        "eqeqeq": 2,
        "indent": ["error", 2],
        "no-console": 0,
        "no-debugger": 1,
        "no-undef": 2,
        "no-var": 2,
        "quotes": [2, "single"],
        "semi": [2, "never"],
        "strict": [2, "function"]
    }
}
